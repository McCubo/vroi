{% extends 'base.html.twig' %}

{% block body %}
    <h1>Amlstation</h1>

    <table>
        <tbody>
            <tr>
                <th>Staname</th>
                <td>{{ amlStation.staName }}</td>
            </tr>
            <tr>
                <th>Stacreateddate</th>
                <td>{% if amlStation.staCreatedDate %}{{ amlStation.staCreatedDate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Staid</th>
                <td>{{ amlStation.staId }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_station_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_station_edit', { 'staId': amlStation.staId }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
