{% extends 'base.html.twig' %}

{% block body %}
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Provider Type: {{ amlProviderType.prtName }}</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <div class="row">
            <div class="col-lg-12">
                <h3>Description: {{ amlProviderType.prtDescription }}</h3>                
                <h4>Created Date: {% if amlProviderType.prtCreatedDate %}{{ amlProviderType.prtCreatedDate|date('Y-m-d H:i:s') }}{% endif %}</h4>
                <h4>Last Updated: {% if amlProviderType.prtUpdatedDate %}{{ amlProviderType.prtUpdatedDate|date('Y-m-d H:i:s') }}{% endif %}</h4>
                <h4>Is Active? : {% if amlProviderType.prtStatus %}Yes{% else %}No{% endif %}</h4>
                <a href="{{ path('admin_provider_type_edit', { 'prtId': amlProviderType.prtId }) }}">
                    <button type="button" class="btn btn-success">Edit</button>
                </a>
                <a href="{{ path('admin_provider_type_index') }}">
                    <button type="button" class="btn btn-info">Back to the list</button>
                </a>
            </div>
        </div>
    </div>
{% endblock %}
