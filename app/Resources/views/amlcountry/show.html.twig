{% extends 'base.html.twig' %}

{% block body %}
    <h1>Amlcountry</h1>

    <table>
        <tbody>
            <tr>
                <th>Couname</th>
                <td>{{ amlCountry.couName }}</td>
            </tr>
            <tr>
                <th>Coucreateddate</th>
                <td>{% if amlCountry.couCreatedDate %}{{ amlCountry.couCreatedDate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Couid</th>
                <td>{{ amlCountry.couId }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_country_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_country_edit', { 'couId': amlCountry.couId }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
