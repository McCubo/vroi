{% extends 'base.html.twig' %}

{% block body %}
    <h1>Amlcity</h1>

    <table>
        <tbody>
            <tr>
                <th>Citname</th>
                <td>{{ amlCity.citName }}</td>
            </tr>
            <tr>
                <th>Citcreateddate</th>
                <td>{% if amlCity.citCreatedDate %}{{ amlCity.citCreatedDate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Citid</th>
                <td>{{ amlCity.citId }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_city_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_city_edit', { 'citId': amlCity.citId }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
